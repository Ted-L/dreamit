#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/testem@3.15.2_handlebars@4.7.8_underscore@1.13.6/node_modules/testem/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/testem@3.15.2_handlebars@4.7.8_underscore@1.13.6/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/testem@3.15.2_handlebars@4.7.8_underscore@1.13.6/node_modules/testem/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/testem@3.15.2_handlebars@4.7.8_underscore@1.13.6/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../testem/testem.js" "$@"
else
  exec node  "$basedir/../testem/testem.js" "$@"
fi
