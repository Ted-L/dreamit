#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules/@rdil/parallel-prettier/dist/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules/@rdil/parallel-prettier/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules/@rdil/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules/@rdil/parallel-prettier/dist/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules/@rdil/parallel-prettier/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules/@rdil/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@rdil+parallel-prettier@3.0.0_prettier@3.5.3/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@rdil/parallel-prettier/dist/index.js" "$@"
else
  exec node  "$basedir/../@rdil/parallel-prettier/dist/index.js" "$@"
fi
