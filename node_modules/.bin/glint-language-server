#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules/@glint/core/bin/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules/@glint/core/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules/@glint/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules/@glint/core/bin/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules/@glint/core/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules/@glint/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/@glint+core@1.4.1-unstable.34c4510_typescript@5.6.3/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@glint/core/bin/glint-language-server.js" "$@"
else
  exec node  "$basedir/../@glint/core/bin/glint-language-server.js" "$@"
fi
