#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/lint-to-the-future@2.6.3_encoding@0.1.13/node_modules/lint-to-the-future/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/lint-to-the-future@2.6.3_encoding@0.1.13/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/lint-to-the-future@2.6.3_encoding@0.1.13/node_modules/lint-to-the-future/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/lint-to-the-future@2.6.3_encoding@0.1.13/node_modules:/Users/tedl/Documents/Project/hugo/dreamit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
