{
  "name": "@glimmer/util",
  "version": "0.93.1",
  "license": "MIT",
  "description": "Common utilities used in Glimmer",
  "repository": "https://github.com/tildeio/glimmer/tree/main/packages/@glimmer/util",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/dev/index.d.cts",
        "default": "./dist/dev/index.cjs"
      },
      "default": {
        "development": {
          "types": "./dist/dev/index.d.ts",
          "default": "./dist/dev/index.js"
        },
        "default": {
          "types": "./dist/prod/index.d.ts",
          "default": "./dist/prod/index.js"
        }
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@glimmer/env": "0.1.7",
    "@glimmer/interfaces": "0.93.0"
  },
  "devDependencies": {
    "@types/qunit": "^2.19.9",
    "eslint": "^8.52.0",
    "publint": "^0.2.5",
    "rollup": "^4.5.1",
    "typescript": "*",
    "@glimmer-workspace/build-support": "1.0.0",
    "@glimmer/constants": "0.92.3",
    "@glimmer/debug-util": "0.92.3",
    "@glimmer/local-debug-flags": "0.92.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "test:lint": "eslint .",
    "test:publint": "publint",
    "test:types": "tsc --noEmit -p ../tsconfig.json"
  }
}